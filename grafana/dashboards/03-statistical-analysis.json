      "targets": [
        {
          "alias": "Batch Processing",
          "datasource": {
            "type": "influxdb",
            "name": "InfluxDB-SeismicData"
          },
          "query": "from(bucket: \"earthquakes\")\n  |> range(start: v.timeRangeStart, stop: v.timeRangeStop)\n  |> filter(fn: (r) => r[\"_measurement\"] == \"daily_global_stats\")\n  |> filter(fn: (r) => r[\"source\"] == \"batch\")\n  |> filter(fn: (r) => r[\"_field\"] == \"eq_count\")\n  |> aggregateWindow(every: 1d, fn: mean, createEmpty: false)",
          "refId": "A"
        },
        {
          "alias": "Stream Processing",
          "datasource": {
            "type": "influxdb",
            "name": "InfluxDB-SeismicData"
          },
          "query": "from(bucket: \"earthquakes\")\n  |> range(start: v.timeRangeStart, stop: v.timeRangeStop)\n  |> filter(fn: (r) => r[\"_measurement\"] == \"hourly_global_stats\")\n  |> filter(fn: (r) => r[\"source\"] == \"stream\")\n  |> filter(fn: (r) => r[\"_field\"] == \"eq_count\")\n  |> aggregateWindow(every: 1m, fn: mean, createEmpty: false)",
          "refId": "B"
        }
      ],
      "title": "Batch vs Stream Processing Comparison",
      "type": "timeseries"
    },
